 <div class= "blog_heading"> Musings</div>

  <div class="posts">
    <% @sorted_posts.each do |post, array|%>
      <span class="post_date"><%= array["date"].strftime("%B %d, %Y:") %><span>
      <a class="post_link" href="/blog/<%= post%>"> <%= array["title"] %></a>
      <br>
    <% end %>
    
  </div>

</body>